<div class="content-wrapper">
    <div class="container-fluid">
        <div class="row">
            <div class="col-12">

                <section class="content">
    
          <!-- Default box -->
          <div class="card">
            <div class="card-header">
              <h3 class="card-title">Delete Device </h3>
    
            </div>
             <% if(success){ %>
               <div class="container">
                  <div class="alert" style="background-color: #FFFFFF;color:blue">
                      <%= success %>
                       <a href="" class="close" data-dismiss="alert" aria-label="close">&times;</a>
                  </div>
               </div>
            <% } %>
            <div class="card-body p-0">
              <div class="module-body table" style="overflow-x: scroll;">
                <table cellpadding="0" cellspacing="0" border="0" class="datatable-1 table table-bordered table-striped  display"
                    width="" >
                    <thead>
                        <tr>
                            <th>
                                S#NO
                            </th>
                            <th>
                                Device Name
                            </th>
                            <th>
                                ON/OFF
                            </th>
                            <th>
                                OPEN / CLOSE
                            </th>
                            <th>
                                Temperature
                            </th>
                            <th>
                                Volume
                            </th>
                            <th>
                                Channel
                            </th>
                            <th>
                                Color
                            </th>
                            <th>
                              Update Status
                            </th>


                        </tr>
                    </thead>
                    <tbody>
                   <% var sr = 1; getAllDevices.forEach(function(Device){ %>
                      <tr>
                        <td><%= sr %></td>
                        <td><a><%= Device.name %></a></td>
                        <% if(Device.onOff == "NotApplicable"){ %>
                        <td><a></a></td>
                        <% }else{ %>
                        <td><a><%= Device.onOff %></a></td>
                        <% } %>
                        <% if(Device.openClose == "NotApplicable"){ %>
                        <td><a></a></td>
                        <% }else{ %>
                        <td><a><%= Device.openClose %></a></td>
                        <% } %>
                         <% if(Device.temperature == "NotApplicable"){ %>
                        <td><a></a></td>
                        <% }else{ %>
                        <td><a><%= Device.temperature %></a></td>
                         <% } %>
                         <% if(Device.volume == "NotApplicable"){ %>
                        <td><a></a></td>
                        <% }else{ %>
                        <td><a><%= Device.volume %></a></td>
                         <% } %>
                          <% if(Device.channel == "NotApplicable"){ %>
                        <td><a></a></td>
                        <% }else{ %>
                        <td><a><%= Device.channel %></a></td>
                         <% } %>
                        <td><a><%= Device.color %></a></td>
                     
                        <td class="project-actions text-right">
                            <a class="btn btn-info btn-sm" href="/delete1-device/<%= Device.id %>">
                                <i class="fas fa-pencil-alt">
                                </i>
                                Delete
                            </a>
                        </td>
                    </tr>
                  <% sr++ }); %>
                    </tbody>
                    <tfoot>
                        <tr>
                           <th>
                                S#NO
                            </th>
                            <th>
                                Device Name
                            </th>
                            <th>
                                ON/OFF
                            </th>
                            <th>
                                OPEN / CLOSE
                            </th>
                            <th>
                                Temperature
                            </th>
                            <th>
                                Volume
                            </th>
                            <th>
                                Channel
                            </th>
                            <th>
                                Color
                            </th>
                            <th>
                              Update Status
                            </th>
                        </tr>
                    </tfoot>
                </table>
            </div>
            </div>
            <!-- /.card-body -->
          </div>
          <!-- /.card -->
    
        </section>

            </div>
